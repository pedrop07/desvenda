// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Answer {
  id     String @id @default("3a41c5dc-bc10-4e79-87f4-6c3d3845d794")
  answer String
}
